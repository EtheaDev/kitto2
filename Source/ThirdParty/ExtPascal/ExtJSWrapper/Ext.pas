    FOnMouseout(TExtButton(ParamAsObject('This')), TEvent(ParamAsObject('E')))
  else if (AEvtName = 'mouseover') and Assigned(FOnMouseover) then
    FOnMouseover(TExtButton(ParamAsObject('This')), TEvent(ParamAsObject('E')))
